	"description": "A front-end improvement to Bootstrap forms with a Material design.",
	"files": [
		"dist/{css,js}/*.{css,js,map}",
		"js/{src,dist}/**/*.{js,map}",
		"scss/**/*.scss"
	],
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/swapform"
	},
	"homepage": "https://swapform.robertjessemiller.com/",
